{
  "name": "react-template-cli",
  "version": "1.0.0",
  "description": "这是一个基于react搭建的脚手架模版",
  "main": "index.js",
  "scripts": {
    "dev": "npm run start",
    "start": "env-cmd -r .env.json -e development node webpack/server",
    "build:qa": "env-cmd -r .env.json -e qa webpack --config ./webpack/config/webpack.prod.js",
    "build:prod": "env-cmd -r .env.json -e production webpack --config ./webpack/config/webpack.prod.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint --config .commitlintrc.js -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js}": [
      "eslint --config .eslintrc.js"
    ],
    "*.{css,less,scss}": [
      "stylelint --config .stylelintrc.js"
    ],
    "*.{ts,tsx,js,json,html,yml,css,less,scss,md}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.23.0",
    "@babel/core": "^7.23.2",
    "@babel/plugin-transform-runtime": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-react": "^7.22.15",
    "@babel/preset-typescript": "^7.23.2",
    "@babel/runtime-corejs3": "^7.23.2",
    "@eslint/js": "^9.2.0",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.19",
    "babel-loader": "^9.1.3",
    "chalk": "^4.1.2",
    "compression-webpack-plugin": "^11.1.0",
    "copy-webpack-plugin": "^12.0.2",
    "core-js": "^3.33.2",
    "css-loader": "^7.1.1",
    "css-minimizer-webpack-plugin": "^7.0.0",
    "detect-port-alt": "^1.1.6",
    "env-cmd": "^10.1.0",
    "error-overlay-webpack-plugin": "^1.1.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.1",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "globals": "^15.2.0",
    "html-webpack-plugin": "^5.6.0",
    "husky": "^9.0.11",
    "ip": "^2.0.1",
    "less": "^4.2.0",
    "less-loader": "^12.2.0",
    "lint-staged": "^15.2.2",
    "mini-css-extract-plugin": "^2.9.0",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-loader": "^8.1.1",
    "postcss-preset-env": "^9.5.13",
    "preload-webpack-plugin": "^3.0.0-beta.4",
    "prettier": "^3.2.5",
    "style-loader": "^3.3.2",
    "stylelint": "^14.16.1",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.7.0",
    "stylelint-order": "^6.0.3",
    "terser-webpack-plugin": "^5.3.10",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.9.0",
    "webpack": "^5.91.0",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "3.11.2",
    "webpack-merge": "^5.10.0",
    "webpackbar": "^6.0.1"
  },
  "dependencies": {
    "antd": "^5.17.2",
    "axios": "^1.6.0",
    "axios-retry": "^3.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.23.1"
  }
}
